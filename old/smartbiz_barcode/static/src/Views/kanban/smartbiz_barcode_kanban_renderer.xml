<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">

    <t t-name="smartbiz_barcode.KanbanRenderer" t-inherit="web.KanbanRenderer">
        <xpath expr="//div[hasclass('o_kanban_renderer')]" position="before">
            <div class=" h5 w-100 mt-4 mb-2 d-flex align-items-center justify-content-center">
                <button  class=" btn fa fa-2x fa-barcode" t-on-click="openManualScanner"/>
            </div>
        </xpath>
    </t>

</templates>
