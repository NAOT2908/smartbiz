<templates xml:space="preserve">
    <t t-name="CustomMainMenu" t-inherit="smartbiz_barcode.MainMenu" t-inherit-mode="extension">
        <xpath expr="//div[@class='main-buttons']" position="inside">
            <button class="button-operations" t-on-click="() => this.openPickingType()">
                Hoạt động
            </button>
            <button class="button-operations" t-on-click="() => this.openPickingBatch()">
                Điều chuyển hàng loạt
            </button>
        </xpath>
    </t>
</templates>